<?xml version="1.0" encoding="utf-8"?>
<package>
  <id>Intent.Typescript.ServiceAgent.Contracts</id>
  <version>1.0.2</version>
  <summary>Typescript DTO contracts for service proxies</summary>
  <description>Typescript DTO contracts for service proxies</description>
  <authors>Intent Software</authors>
  <templates>
    <template id="Intent.Typescript.ServiceAgent.Contracts.DTO.Local">
      <role>Presentation.Web</role>
    </template>
    <template id="Intent.Typescript.ServiceAgent.Contracts.DTO.Remote">
      <role>Presentation.Web</role>
      <location>wwwroot\App\DTOs\Generated\${Application.Name}</location>
      <config>
        <add key="Namespace" default="App.Contracts.${Application.Name}"/>
      </config>
    </template>
  </templates>
  <decorators>
  </decorators>
  <dependencies>
  </dependencies>
  <files>
    <file src="bin\$configuration$\$id$.dll"/>
    <file src="bin\$configuration$\$id$.pdb"/>
  </files>
</package>