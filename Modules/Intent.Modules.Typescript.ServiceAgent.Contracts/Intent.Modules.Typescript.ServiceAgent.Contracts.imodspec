<?xml version="1.0" encoding="utf-8"?>
<package>
  <id>Intent.Typescript.ServiceAgent.Contracts</id>
  <version>3.0.0-beta.3</version>
  <supportedClientVersions>[3.0.0-beta,4.0.0)</supportedClientVersions>
  <summary>Typescript DTO contracts for service proxies</summary>
  <description>Typescript DTO contracts for service proxies</description>
  <authors>Intent Architect</authors>
  <templates>
    <template id="Intent.Typescript.ServiceAgent.Contracts.DTO.Local">
      <role>Presentation.Web</role>
      <location>wwwroot/App/DTOs/Generated</location>
    </template>
    <template id="Intent.Typescript.ServiceAgent.Contracts.DTO.Remote">
      <role>Presentation.Web</role>
      <location>wwwroot/App/DTOs/Generated</location>
      <config>
        <add key="Consumer Stereotype Name"
             default="Consumers"
             description="The name of Stereotype to search for on DTOs in a different applications which contain the 'Consumer Stereotype Property Name' specified below." />
        <add key="Consumer Stereotype Property Name"
             default="TypeScript"
             description="The name of the Stereotype Property on the above 'Consumer Stereotype Name' containing a comma separated list of which are 'consuming' applications for the stereotyped DTO." />
      </config>
    </template>
  </templates>
  <decorators>
  </decorators>
  <dependencies>
    <dependency id="Intent.Common.TypeScript" version="3.0.0-beta.3" />
    <dependency id="Intent.Common.Types" version="3.0.0-beta.3"/>
  </dependencies>
  <files>
    <file src="$outDir$\$id$.dll" />
    <file src="$outDir$\$id$.pdb" />
  </files>
</package>