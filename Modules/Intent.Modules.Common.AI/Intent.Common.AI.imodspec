<?xml version="1.0" encoding="utf-8"?>
<package>
  <id>Intent.Common.AI</id>
  <version>1.0.0-beta.2</version>
  <supportedClientVersions>[4.5.0-a,5.0.0)</supportedClientVersions>
  <summary>Support for accessing LLMs via Semantic Kernel within Intent Architect module tasks.</summary>
  <description>Support for accessing LLMs via Semantic Kernel within Intent Architect module tasks.</description>
  <iconUrl>data:image/png;base64,</iconUrl>
  <authors>Intent Architect</authors>
  <tags>gpt ai llm common semantickernel</tags>
  <releaseNotes>release-notes.md</releaseNotes>
  <templates></templates>
  <decorators></decorators>
  <factoryExtensions></factoryExtensions>
  <moduleSettings>
    <group id="62594c9b-21fe-4c65-ac5c-b32a836a2ca5" title="AI Settings" type="user-settings" externalReference="62594c9b-21fe-4c65-ac5c-b32a836a2ca5">
      <settings>
        <setting id="41ac1bce-6362-4b77-b588-ba3df55a2afb" title="Provider" type="select">
          <defaultValue>open-ai</defaultValue>
          <isRequiredFunction>return true;</isRequiredFunction>
          <options>
            <option value="open-ai" description="Open AI" />
            <option value="azure-open-ai" description="Azure Open AI" />
            <option value="ollama" description="Ollama (Experimental)" />
            <option value="anthropic" description="Anthropic" />
          </options>
        </setting>
        <setting id="9e9a32b4-194e-4d53-b62c-c9c28fb7b6f8" title="Open AI API Key" type="text">
          <hint>Will fall back to environment variable if empty.</hint>
          <isActiveFunction>let provider = getProperty("Provider").value;
return provider === "open-ai";</isActiveFunction>
        </setting>
        <setting id="d76d0a4d-60c5-42b4-aa3b-de4f11b44ac1" title="Azure Open AI API Key" type="text">
          <hint>Will fall back to environment variable if empty.</hint>
          <isActiveFunction>let provider = getProperty("Provider").value;
return provider === "azure-open-ai";</isActiveFunction>
        </setting>
        <setting id="715e2ce8-677c-467d-a876-8dc84b99ae05" title="Anthropic API Key" type="text">
          <hint>Will fall back to environment variable if empty.</hint>
          <isActiveFunction>let provider = getProperty("Provider").value;
return provider === "anthropic";</isActiveFunction>
        </setting>
        <setting id="effcfd41-aaed-4278-a7e7-818009584139" title="Model" type="text">
          <defaultValue>gpt-4o</defaultValue>
          <isRequiredFunction>return true;</isRequiredFunction>
        </setting>
        <setting id="5d2a1254-9f21-4cbd-818e-497bf09c87ea" title="API Url" type="text">
          <hint>URL to connect to AI API.</hint>
          <defaultValue>http://localhost:11434</defaultValue>
          <isActiveFunction>let provider = getProperty("Provider").value;
return provider === "ollama" || provider === "azure-open-ai";</isActiveFunction>
          <isRequiredFunction>return true;</isRequiredFunction>
        </setting>
        <setting id="07a3313b-0602-4f83-a1c1-820e746bde48" title="Deployment name" type="text">
          <isActiveFunction>let provider = getProperty("Provider").value;
return provider === "azure-open-ai";</isActiveFunction>
          <isRequiredFunction>return true;</isRequiredFunction>
        </setting>
        <setting id="4cb52ca6-af4f-4dbf-9f75-5a74438cd281" title="Max Tokens" type="text">
          <defaultValue>8192</defaultValue>
          <isActiveFunction>let provider = getProperty("Provider").value;
return provider === "anthropic";</isActiveFunction>
          <isRequiredFunction>return true;</isRequiredFunction>
        </setting>
      </settings>
    </group>
  </moduleSettings>
  <dependencies>
    <dependency id="Intent.Common" version="3.7.2" />
    <dependency id="Intent.Common.Types" version="3.4.0" />
  </dependencies>
