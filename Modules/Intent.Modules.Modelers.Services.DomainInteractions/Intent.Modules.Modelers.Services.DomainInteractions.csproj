<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Version>2.3.12</Version>
    <Authors>Intent Architect</Authors>
    <Company>Intent Architect</Company>
    <Product>Intent Architect</Product>
    <Copyright>Copyright 2023</Copyright>
    <PackageProjectUrl>https://intentarchitect.com</PackageProjectUrl>
    <PackageTags>Intent Architect Software Factory SDK Modules Services DomainInteractions</PackageTags>
    <RootNamespace>Intent.Modelers.Services.DomainInteractions</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIcon>logo.png</PackageIcon>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\logo.png" Link="logo.png">
      <PackagePath></PackagePath>
      <Pack>True</Pack>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Intent.Modules.Common" Version="3.8.0" />
    <PackageReference Include="Intent.Modules.Common.CSharp" Version="3.8.1" />
    <PackageReference Include="Intent.Modules.Common.Types" Version="4.0.0" />
    <PackageReference Include="Intent.Modules.Modelers.Domain" Version="3.12.6" />
    <PackageReference Include="Intent.Modules.Modelers.Services" Version="4.0.5" />
    <PackageReference Include="Intent.Modules.Modelers.Services.CQRS" Version="6.0.5" />
    <PackageReference Include="Intent.Packager" Version="3.6.1">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Intent.Persistence.SDK" Version="1.0.2" />
    <PackageReference Include="Intent.RoslynWeaver.Attributes" Version="3.0.0" />
    <PackageReference Include="Intent.SoftwareFactory.SDK" Version="3.9.0-alpha.4" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="modelers\Services Domain Interactions.designer.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Services Domain Interactions.designer.settings</DependentUpon>
    </Compile>
  </ItemGroup>

</Project>