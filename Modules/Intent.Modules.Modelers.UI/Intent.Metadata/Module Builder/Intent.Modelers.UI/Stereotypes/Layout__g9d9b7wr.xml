  <displayFunction>const display = [];
const process = (property) =&gt; {
    if (getProperty(property).getValue()) {
        display.push(`${property}="${getProperty(property).getValue()}"`)
    }
}

process("xs");
process("sm");
process("md");
process("lg");
process("xl");
process("xxl");

return `[${display.join(" ")}]`;</displayFunction>
