  <displayFunction>var macOsShortcut = properties["Shortcut (macOS)"].value;
var shortcut = properties["Shortcut"].value;

if (!shortcut) {
    return "[No Shortcut]";
}

if (macOsShortcut) {
    return `[${shortcut} / ${macOsShortcut}]`
}

return `[${shortcut}]`;</displayFunction>
