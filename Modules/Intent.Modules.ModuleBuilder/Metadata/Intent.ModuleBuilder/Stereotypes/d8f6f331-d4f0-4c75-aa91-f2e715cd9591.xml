      <controlType>checkbox</controlType>
      <optionsSource>n/a</optionsSource>
      <placeholder />
      <defaultValue>true</defaultValue>
      <valueOptions />
      <lookupTypes />
      <isActiveFunction>property['Represents'].value == 'Reference' &amp;&amp; (property['Mode'].value == 'Optional' || property['Mode'].value == 'Required')</isActiveFunction>
