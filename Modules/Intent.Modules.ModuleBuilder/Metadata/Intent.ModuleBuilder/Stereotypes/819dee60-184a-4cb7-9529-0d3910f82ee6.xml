<?xml version="1.0" encoding="utf-8"?>
<stereotypeDefinition id="819dee60-184a-4cb7-9529-0d3910f82ee6">
  <name>File Template Settings</name>
  <icon type="UrlImagePath" source="data:image/png;base64," />
  <displayIcon>false</displayIcon>
  <displayFunction>
    [${CreationMode}${ CreationMode == 'File per Model' || CreationMode == 'Single File (Model List)' ? ': ' + ifError(() =&gt; lookup(Modeler).label + "-&gt;" + lookup(Modeler).child(ModelType).label, "&lt;not selected&gt;") : '' }]
  </displayFunction>
  <targetElements>
    <element>c# template</element>
    <element>file template</element>
  </targetElements>
  <autoAdd>true</autoAdd>
  <parentFolderId>bc39b8bc-6c2c-4724-bf5c-09bb439e0f26</parentFolderId>
  <properties>
    <property id="311d788f-7582-4c27-9a4d-e0db1439c327">
      <name>Creation Mode</name>
      <type>options</type>
      <controlType>select</controlType>
      <optionsSource>options</optionsSource>
      <placeholder />
      <defaultValue>Single File (No Model)</defaultValue>
      <valueOptions>
        <option>Single File (No Model)</option>
        <option>Single File (Model List)</option>
        <option>File per Model</option>
        <option>Custom</option>
      </valueOptions>
      <lookupTypes />
      <isRequiredFunction>true</isRequiredFunction>
    </property>
    <property id="c2283d15-4b57-4ed9-8aae-6efc73039034">
      <name>Modeler</name>
      <type>lookup-type</type>
      <controlType>select</controlType>
      <optionsSource>lookup-element</optionsSource>
      <placeholder />
      <defaultValue />
      <valueOptions />
      <lookupTypes>
        <type>Modeler Reference</type>
        <type>Modeler</type>
        <type>Modeler Extension</type>
      </lookupTypes>
      <isActiveFunction>property['Creation Mode'].value != 'Single File (No Model)'</isActiveFunction>
      <isRequiredFunction>property['Creation Mode'].value != 'Single File (No Model)'</isRequiredFunction>
    </property>
    <property id="987590ee-a5ca-45ce-b742-0ee764b6b6e9">
      <comment>In the event that a 'Custom' Modeler is chosen, specify the desired Type to use ('object' by default).</comment>
      <name>Model Type</name>
      <type>internal-lookup</type>
      <controlType>select</controlType>
      <optionsSource>nested-lookup</optionsSource>
      <placeholder />
      <defaultValue />
      <valueOptions />
      <lookupTypes />
      <lookupInternalTargetPropertyId>c2283d15-4b57-4ed9-8aae-6efc73039034</lookupInternalTargetPropertyId>
      <isActiveFunction>property['Creation Mode'].value != 'Single File (No Model)'</isActiveFunction>
      <isRequiredFunction>property['Creation Mode'].value != 'Single File (No Model)'</isRequiredFunction>
    </property>
    <property id="329d0550-b81b-4db6-a99b-05d90360d432">
      <name>File Extension</name>
      <type>string</type>
      <controlType>text</controlType>
      <optionsSource>n/a</optionsSource>
      <placeholder>(required)</placeholder>
      <defaultValue>txt</defaultValue>
      <valueOptions />
      <lookupTypes />
      <isRequiredFunction />
    </property>
  </properties>
</stereotypeDefinition>