                <property name="73a4088f-626e-4d31-b0b2-113167f015b2" display="Script" value="return; // disabled but left here as a reference. This logic must be moved to the approriate place&#xD;&#xA;&#xD;&#xA;const commandMappingSettingId = &quot;e4e36072-a2ce-42d3-93c5-0b496d79ef43&quot;;&#xD;&#xA;if (element.getMapping() &amp;&amp; element.getMapping().mappingSettingsId == commandMappingSettingId) {&#xD;&#xA;    handleCommandQueryStandaloneType(&quot;command&quot;);&#xD;&#xA;&#xD;&#xA;    let stereotype = element.getStereotype(&quot;Http Settings&quot;);&#xD;&#xA;    if (stereotype) {&#xD;&#xA;        setIfEmpty(stereotype, &quot;Verb&quot;, &quot;POST&quot;);&#xD;&#xA;        setIfEmpty(stereotype, &quot;Route&quot;, &quot;[action]&quot;);&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;const queryMappingSettingId = &quot;f15fa935-434e-4983-b283-25cde2cb440e&quot;;&#xD;&#xA;if (element.getMapping() &amp;&amp; element.getMapping().mappingSettingsId == queryMappingSettingId) {&#xD;&#xA;    handleCommandQueryStandaloneType(&quot;query&quot;);&#xD;&#xA;&#xD;&#xA;    let stereotype = element.getStereotype(&quot;Http Settings&quot;);&#xD;&#xA;    if (stereotype) {&#xD;&#xA;        setIfEmpty(stereotype, &quot;Verb&quot;, &quot;GET&quot;);&#xD;&#xA;        setIfEmpty(stereotype, &quot;Route&quot;, &quot;[action]&quot;);&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;// Camelcases all parameters&#xD;&#xA;for(var param of element.getChildren(&quot;Parameter&quot;)) {&#xD;&#xA;    param.setName(param.name[0].toLowerCase() + param.name.substring(1));&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;function handleCommandQueryStandaloneType(parameterName) {&#xD;&#xA;    let commandQuery = element.getMapping().getElement();&#xD;&#xA;    let mappedFields = element.getChildren(&quot;Parameter&quot;).filter(x =&gt; x.isMapped &amp;&amp; x.typeReference.getTypeId() != commandQuery.id);&#xD;&#xA;    let hasCommandQueryParam = element.getChildren(&quot;Parameter&quot;).some(x =&gt; x.typeReference.getTypeId() == commandQuery.id);&#xD;&#xA;&#xD;&#xA;    if (mappedFields.length &lt; commandQuery.getChildren().length &amp;&amp; !hasCommandQueryParam) {&#xD;&#xA;        let param = createElement(&quot;Parameter&quot;, parameterName, element.id);&#xD;&#xA;        param.typeReference.setType(commandQuery.id);&#xD;&#xA;        return;&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    if (mappedFields.length == commandQuery.getChildren().length &amp;&amp; hasCommandQueryParam) {&#xD;&#xA;        let foundElements = element.getChildren(&quot;Parameter&quot;).filter(x =&gt; x.typeReference.getTypeId() == commandQuery.id)&#xD;&#xA;        foundElements.forEach(x =&gt; { x.delete(); });&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;function setIfEmpty(stereotype, propertyName, defaultValue) {&#xD;&#xA;    let prop = stereotype?.getProperty(propertyName);&#xD;&#xA;    if (prop &amp;&amp; !prop.getValue() &amp;&amp; prop.getValue() != &quot;&quot;) {&#xD;&#xA;        prop.setValue(defaultValue);&#xD;&#xA;    }&#xD;&#xA;}" isActive="true" />
