<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Version>6.0.4</Version>
    <Authors>Intent Architect</Authors>
    <Company>Intent Architect</Company>
    <Product>Intent Architect</Product>
    <Copyright>Copyright 2021</Copyright>
    <PackageProjectUrl>https://intentarchitect.com</PackageProjectUrl>
    <PackageIconUrl></PackageIconUrl>
    <PackageTags>Intent Architect Software Factory SDK Modules Services Modeler</PackageTags>
    <RootNamespace>Intent.Modelers.Services</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIcon>logo.png</PackageIcon>
    <RepositoryUrl>https://github.com/IntentSoftware/Intent.Modules.NET</RepositoryUrl>
    <Description>CQRS Extensions for the standard Services designer</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Intent.Modules.Common" Version="3.8.0" />
    <PackageReference Include="Intent.Modules.Common.Types" Version="3.4.0" />
    <PackageReference Include="Intent.Modules.Modelers.Services" Version="4.0.0" />
    <PackageReference Include="Intent.Packager" Version="3.6.1">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Intent.RoslynWeaver.Attributes" Version="3.0.0" />
    <PackageReference Include="Intent.SoftwareFactory.SDK" Version="3.7.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>