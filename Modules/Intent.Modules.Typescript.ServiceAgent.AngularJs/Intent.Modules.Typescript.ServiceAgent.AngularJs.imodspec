<?xml version="1.0" encoding="utf-8"?>
<package>
  <id>Intent.Typescript.ServiceAgent.AngularJs</id>
  <version>2.0.1</version>
  <summary>Typescript service proxies for AngularJS</summary>
  <description>Typescript service proxies for AngularJS</description>
  <authors>Intent Architect</authors>
  <projectUrl>www.intentarchitect.com</projectUrl>
  <tags>AngularJs Typescript Proxy ServiceAgent</tags>
  <templates>
    <template id="Intent.Typescript.ServiceAgent.AngularJs.Proxy.Local">
      <role>Presentation.Web</role>
      <config>
        <add key="Namespace"
             default="App.Proxies"/>
        <add key="AngularModule"
             description="AngularJs module to register this proxy into."
             default="App" />
      </config>
    </template>
    <template id="Intent.Typescript.ServiceAgent.AngularJs.Proxy.Remote">
      <role>Presentation.Web</role>
      <config>
        <add key="Namespace"
             default="App.Proxies.${Model.Application.Name}"/>
        <add key="AngularModule"
             default="App" description="AngularJs module to register this proxy into." />
        <add key="Consumer Stereotype Name"
             default="Consumers"
             description="The name of Stereotype to search for on services in a different applications which contain the 'Consumer Stereotype Property Name' specified below." />
        <add key="Consumer Stereotype Property Name"
             default="CommaSeperatedList"
             description="The name of the Stereotype Property on the above 'Consumer Stereotype Name' containing a comma separated list of which are 'consuming' applications for the stereotyped service." />
      </config>
    </template>
  </templates>
  <decorators>
  </decorators>
  <dependencies>
    <dependency id="Intent.Common" version="2.0.0"/>
    <dependency id="Intent.Typescript.ServiceAgent.Contracts" version="2.0.0"/>
  </dependencies>
  <files>
    <file src="bin\$configuration$\$id$.dll"/>
    <file src="bin\$configuration$\$id$.pdb"/>
  </files>
</package>