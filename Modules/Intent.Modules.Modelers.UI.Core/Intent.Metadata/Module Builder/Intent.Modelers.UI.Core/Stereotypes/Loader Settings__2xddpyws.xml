  <onAppliedScript>const dataBindingMappingTypeId = "ee19a932-0cea-4b90-bd3c-857d7942d390";

const component = element.getParent();
if (component.getChildren().every(x =&gt; x.getName() != "IsLoading")) {
    const isLoadingProperty = createElement("Property", "IsLoading", component.id);
    isLoadingProperty.typeReference.setType("e6f92b09-b2c5-4536-8270-a4d9e5bbd930"); // bool
    component.getAdvancedMapping().addMappedEnd(dataBindingMappingTypeId, [component.id, isLoadingProperty.id], [element.id, "2cfd43b2-2a18-4ac0-8cf3-d1aec9d7e699"]);
}

if (component.getChildren().every(x =&gt; x.getName() != "ErrorMessage")) {
    const errorMessageProperty = createElement("Property", "ErrorMessage", component.id);
    errorMessageProperty.typeReference.setIsNullable(true);
    component.getAdvancedMapping().addMappedEnd(dataBindingMappingTypeId, [component.id, errorMessageProperty.id], [element.id, "2e482e27-b176-43cf-b80a-33123036142a"]);
}
