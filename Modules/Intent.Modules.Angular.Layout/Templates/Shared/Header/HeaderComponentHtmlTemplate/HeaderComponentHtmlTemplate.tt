<#@ template language="C#" inherits="HtmlTemplateBase<object>" #>
<#@ assembly name="System.Core" #>
<#@ import namespace="System.Collections.Generic" #>
<#@ import namespace="System.Linq" #>
<#@ import namespace="Intent.Modules.Common" #>
<#@ import namespace="Intent.Modules.Common.Templates" #>
<#@ import namespace="Intent.Modules.Common.Html.Templates" #>
<#@ import namespace="Intent.Metadata.Models" #>
<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <a class="navbar-brand" routerLink="/">
    <img width="30" height="30" class="d-inline-block align-top" alt="" src="data:image/svg+xml;base64,">
    {{ title }}
  </a>
  <button class="navbar-toggler" type="button" (click)="isCollapsed = !isCollapsed" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" [collapse]="isCollapsed">
    <ul class="navbar-nav" intent-manage intent-id="navbar">
<#  foreach (var moduleRoute in _mainRoutes) { #>
      <li class="nav-item active" intent-manage="<#= moduleRoute.Route #>">
        <a class="nav-link" routerLink="/<#= moduleRoute.Route #>"><#= moduleRoute.ModuleName #></a>
      </li>
<#  } #>
    </ul>
  </div>
</nav>
