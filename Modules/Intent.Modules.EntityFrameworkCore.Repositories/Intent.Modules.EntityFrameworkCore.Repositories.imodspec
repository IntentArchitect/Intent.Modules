<?xml version="1.0" encoding="utf-8"?>
<package>
  <id>Intent.EntityFrameworkCore.Repositories</id>
  <version>1.5.0</version>
  <summary>Repository Patterns for Entity Framework Core.</summary>
  <description>Repository Patterns for Entity Framework Core.</description>
  <authors>Intent Software</authors>
  <projectUrl>https://intentarchitect.com/</projectUrl>
  <tags>EntityFrameworkCore Repositories EntityFramework Core</tags>
  <templates>
    <template id="Intent.EntityFrameworkCore.Repositories.Implementation">
      <role>Repository.Implementation</role>
      <config>
