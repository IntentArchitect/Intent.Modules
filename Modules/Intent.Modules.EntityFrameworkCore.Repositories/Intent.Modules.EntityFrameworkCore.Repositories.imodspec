      </config>
    </template>
  </templates>
  <decorators>
  </decorators>
  <dependencies>
    <dependency id="Intent.Common" version="1.5.0"/>
    <dependency id="Intent.Common.Types" version="1.5.0"/>
    <dependency id="Intent.OutputManager.RoslynWeaver" version="1.5.0"/>
    <dependency id="Intent.Entities.Repositories.Api" version="1.5.0"/>
    <dependency id="Intent.EntityFrameworkCore" version="1.5.0"/>
  </dependencies>
  <files>
    <file src="bin\$configuration$\$id$.dll"/>
    <file src="bin\$configuration$\$id$.pdb"/>
  </files>
</package>