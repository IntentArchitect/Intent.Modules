  <templates>
    <template id="Intent.Entities.Keys.IdentityGenerator">
      <role>Domain.Common</role>
    </template>
  </templates>
  <decorators>
    <decorator id="Intent.Entities.Keys.SurrogatePrimaryKeyInterfaceDecorator"  enabled="true">
      <config>
        <add key="Surrogate Key Type" description="The type used for the implicit surrogate keys." default="Guid" />
      </config>
    </decorator>
    <decorator id="Intent.Entities.Keys.SurrogatePrimaryKeyEntityStateDecorator"  enabled="true">
      <config>
        <add key="Surrogate Key Type" description="The type used for the implicit surrogate keys." default="Guid" />
      </config>
    </decorator>
    <decorator id="Intent.Entities.Keys.ForeignKeyEntityInterfaceDecorator"  enabled="true">
      <config>
        <add key="Foreign Key Type" description="The type used for the foreign keys. Should match the surrogate key types." default="Guid" />
      </config>
    </decorator>
    <decorator id="Intent.Entities.Keys.ForeignKeyEntityDecorator"  enabled="true">
      <config>
        <add key="Foreign Key Type" description="The type used for the foreign keys. Should match the surrogate key types." default="Guid" />
      </config>
    </decorator>
    <decorator id="Intent.Entities.Keys.BidirectionalOneToManyEntityDecorator"  enabled="true"/>
  </decorators>
  <dependencies>
    <dependency id="Intent.Entities" version="3.0.0-beta.1"/>
    <dependency id="Intent.Metadata.RDBMS" version="3.0.0-beta.1"/>
  </dependencies>
  <files>
    <file src="bin/$configuration$/$id$.dll"/>
    <file src="bin/$configuration$/$id$.pdb"/>
  </files>
</package>