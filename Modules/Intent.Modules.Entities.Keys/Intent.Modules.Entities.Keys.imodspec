<?xml version="1.0" encoding="utf-8"?>
<package>
  <id>Intent.Entities.Keys</id>
  <version>3.0.0-beta.1</version>
  <supportedClientVersions>[3.0.0-beta,4.0.0)</supportedClientVersions>
  <summary>Adds primary and foreign key properties to entities.</summary>
  <description>Adds primary and foreign key properties to entities. This is typically required for ORMs (Object Relational Mappers) such as Entity Framework.</description>
  <authors>Intent Architect</authors>
  <iconUrl>data:image/png;base64,</iconUrl>
