<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Intent.Modules.Common" Version="3.3.16" />
    <PackageReference Include="Intent.Modules.Common.CSharp" Version="3.3.24" />
    <PackageReference Include="Intent.Modules.Common.Types" Version="3.3.6" />
    <PackageReference Include="Intent.Modules.ModuleBuilder" Version="3.3.11" />
    <PackageReference Include="Intent.Packager" Version="3.3.2">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Intent.RoslynWeaver.Attributes" Version="2.0.1" />
    <PackageReference Include="Intent.SoftwareFactory.SDK" Version="3.3.3" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Intent.Modules.ModuleBuilder" />
    <None Remove="Api\Extensions\" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Api\Extensions\" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Templates\Templates\DartTemplatePartial\DartTemplatePartialTemplate.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>DartTemplatePartialTemplate.tt</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Templates\Templates\DartTemplatePartial\DartTemplatePartialTemplate.tt">
      <Generator>TextTemplatingFilePreprocessor</Generator>
      <LastGenOutput>DartTemplatePartialTemplate.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>