<?xml version="1.0" encoding="utf-8"?>
<package>
  <id>Intent.Application.Contracts.Mappings</id>
  <version>3.0.0</version>
  <supportedClientVersions>[3.0.0,4.0.0)</supportedClientVersions>
  <summary>Creates mapping configuration and extension methods using Automapper.</summary>
  <description>Creates mapping configuration and extension methods using Automapper.</description>
  <iconUrl>data:image/png;base64,</iconUrl>
  <authors>Intent Architect</authors>
  <templates>    
    <template id="Intent.Application.Contracts.Mapping" enabled="true">
      <role>Application.Mappings</role>
      <config>          
        <add key="ContractTemplateDependancyId" default="Intent.Application.Contracts.DTO" description="The template Id for the template producing the Contracts" />
        <add key="DomainTemplateDependancyId" default="Intent.Entities.DomainEntityInterface" description="The template Id for the template producing the Domain Entities" />
        <add key="Stereotype Name" default="Mapping Configuration" description="The name of the stereotype on a mapped DTO to specify an override source type to map from." />
        <add key="Stereotype Property Name (Type)" default="Type" description="The Type for the above stereotype to specify the source type name to map from." />
        <add key="Stereotype Property Name (Namespace)" default="Namespace" description="The Namespace for the above stereotype to specify the source type's namespace to map from." />
      </config>
    </template>
    <template id="Intent.Application.Contracts.Mapping.Profile">
      <role>Application.Mappings</role>
      <config>
        <add key="ContractTemplateDependancyId" description="The template Id for the template producing the Contracts" default="Intent.Application.Contracts.DTO"  />
        <add key="DomainTemplateDependancyId" description="The template Id for the template producing the Domain Entities" default="Intent.Entities.DomainEntityInterface" />
        <add key="Stereotype Name" default="Mapping Configuration" description="The name of the stereotype on a mapped DTO to specify an override source type to map from." />
        <add key="Stereotype Property Name (Type)" default="Type" description="The Type for the above stereotype to specify the source type name to map from." />
        <add key="Stereotype Property Name (Namespace)" default="Namespace" description="The Namespace for the above stereotype to specify the source type's namespace to map from." />
      </config>
    </template>
  </templates>
  <decorators />
  <dependencies>
    <dependency id="Intent.Common" version="3.0.0"/>
    <dependency id="Intent.OutputManager.RoslynWeaver" version="3.0.0"/>
  </dependencies>
  <files>
    <file src="bin/$configuration$/$id$.dll"/>
    <file src="bin/$configuration$/$id$.pdb"/>
  </files>
</package>